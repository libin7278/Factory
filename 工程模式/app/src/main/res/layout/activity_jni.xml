<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.libin.factory.activity.JNIActivity"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/btn_c_add"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Java调用c执行加法运算"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/btn_c_string"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/btn_c_string"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Java调c执行字符串运算"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_c_add"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_c_add"
        tools:layout_constraintBaseline_creator="1" />

    <Button
        android:id="@+id/btn_c_array"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Java调用c执行数组运算"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_c_secret"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_c_string" />

    <Button
        android:id="@+id/btn_c_secret"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="Java调用c执行密码检查"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btn_c_array"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toTopOf="@+id/btn_c_array" />

    <Button
        android:id="@+id/btn_p_s"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="传入people集合返回修改后的students集合"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_update_std"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/btn_stu_info"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="创建Student对象"
        app:layout_constraintRight_toLeftOf="@+id/btn_update_std"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="4dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_c_secret" />

    <Button
        android:id="@+id/btn_update_std"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="更新Student信息"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_stu_info"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_c_array" />

    <Button
        android:id="@+id/button6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Button"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_p_s"
        tools:layout_constraintLeft_creator="1" />
</android.support.constraint.ConstraintLayout>
